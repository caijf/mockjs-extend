{
  "name": "mockjs-extend",
  "version": "1.2.2",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "jest --verbose",
    "build": "npm run build:cjs && npm run build:es && npm run build:types",
    "build:cjs": "rm -rf ./cjs && tsc -p tsconfig.build.json --outDir cjs -m commonjs",
    "build:es": "rm -rf ./es && tsc -p tsconfig.build.json --outDir es -m esnext",
    "build:types": "rm -rf ./types && tsc -p tsconfig.build.json --outDir types -d --emitDeclarationOnly",
    "prepublishOnly": "npm test && npm run build",
    "tsc": "tsc --noEmit",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "npm run lint:js -- --fix",
    "prettier": "prettier --write **/*",
    "prepare": "husky"
  },
  "files": [
    "cjs",
    "es",
    "types"
  ],
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint",
    "**/*.{css,scss,less,js,jsx,ts,tsx,json,md}": "prettier -w"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "repository": "git@github.com:caijf/mockjs-extend.git",
  "author": "caijf <369756941@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@types/jest": "^29.5.12",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "inquirer": "^9.2.19",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "bankcard": "^3.1.1",
    "intl-tel-code": "^1.1.0",
    "lcn": "^5.0.6",
    "mockjs": "^1.1.0",
    "util-helpers": "^4.21.5"
  },
  "peerDependencies": {
    "@types/mockjs": "^1.0.7"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
