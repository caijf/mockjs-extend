{
  "name": "mockjs-extend",
  "version": "1.6.0",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "jest --verbose",
    "build": "npm run build:cjs && npm run build:es && npm run build:types",
    "build:cjs": "rm -rf ./cjs && tsc -p tsconfig.build.json --outDir cjs -m commonjs",
    "build:es": "rm -rf ./es && tsc -p tsconfig.build.json --outDir es -m esnext",
    "build:types": "rm -rf ./types && tsc -p tsconfig.build.json --outDir types -d --emitDeclarationOnly",
    "prepublishOnly": "npm test && npm run build",
    "tsc": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write **/*",
    "prepare": "husky"
  },
  "files": [
    "cjs",
    "es",
    "types"
  ],
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint",
    "**/*.{css,scss,less,js,jsx,ts,tsx,json,md}": "prettier -w"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "repository": "git@github.com:caijf/mockjs-extend.git",
  "author": "caijf <369756941@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/cz-commitlint": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.19.9",
    "commitizen": "^4.3.1",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "bankcard": "^3.1.2",
    "intl-tel-code": "^1.1.1",
    "lcn": "^7.1.0",
    "mockjs": "^1.1.0",
    "tslib": "^2.8.1",
    "ut2": "^1.20.1",
    "util-helpers": "^5.7.4"
  },
  "peerDependencies": {
    "@types/express": ">=4.0.0",
    "@types/mockjs": "^1.0.7"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
