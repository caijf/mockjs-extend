{
  "name": "mockjs-extend",
  "version": "1.1.11",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "jest --verbose",
    "build": "npm run build:cjs && npm run build:es && npm run build:types",
    "build:cjs": "rm -rf ./cjs && tsc -p tsconfig.build.json --outDir cjs -m commonjs",
    "build:es": "rm -rf ./es && tsc -p tsconfig.build.json --outDir es -m esnext",
    "build:types": "rm -rf ./types && tsc -p tsconfig.build.json --outDir types -d --emitDeclarationOnly",
    "prepublishOnly": "npm test && npm run build",
    "tsc": "tsc --noEmit",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "npm run lint:js -- --fix",
    "prettier": "prettier --write **/*",
    "prepare": "husky install"
  },
  "files": [
    "cjs",
    "es",
    "types"
  ],
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint",
    "**/*.{css,scss,less,js,jsx,ts,tsx,json,md}": "prettier -w"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": "git@github.com:caijf/mockjs-extend.git",
  "author": "caijf <369756941@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/jest": "^29.5.2",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^20.3.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.49.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "bankcard": "^3.0.5",
    "intl-tel-code": "^1.1.0",
    "lcn": "^5.0.3",
    "mockjs": "^1.1.0",
    "util-helpers": "^4.18.0"
  },
  "peerDependencies": {
    "@types/mockjs": "^1.0.7"
  }
}
