{
  "name": "mockjs-extend",
  "version": "1.1.7",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "scripts": {
    "test": "jest --verbose",
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "rm -rf ./cjs && tsc -p tsconfig.build.json --outDir cjs -m commonjs",
    "build:es": "rm -rf ./es && tsc -p tsconfig.build.json --outDir es -m esnext",
    "prepublishOnly": "npm test && npm run build",
    "tsc": "tsc --noEmit"
  },
  "files": [
    "cjs",
    "es"
  ],
  "repository": "git@github.com:caijf/mockjs-extend.git",
  "author": "caijf <369756941@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^20.3.0",
    "ts-jest": "^29.1.0",
    "jest": "^29.5.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "bankcard": "^3.0.5",
    "intl-tel-code": "^1.1.0",
    "lcn": "^5.0.3",
    "mockjs": "^1.1.0",
    "util-helpers": "^4.18.0"
  }
}
